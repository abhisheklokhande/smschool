<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.smschool">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />


    <application
        android:allowBackup="true"
        android:icon="@drawable/schoollogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/schoollogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".editprofile"></activity>
        <activity android:name=".forgetpassword" />
        <activity android:name=".registration" />
        <activity android:name=".feedback" />
        <activity android:name=".aboutus" />
        <activity android:name=".performance" />
        <activity android:name=".events" />
        <activity android:name=".testschedule" />
        <activity android:name=".elearning" />
        <activity android:name=".attendance" />
        <activity android:name=".fee" />
        <activity android:name=".sports" />
        <activity android:name=".canteen" />
        <activity
            android:name=".bustrack"
            android:label="@string/title_activity_bustrack"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".socialActivity" />
        <activity android:name=".holidays" />
        <activity android:name=".dashboard" />
        <activity android:name=".profile" />
        <activity android:name=".MainActivity" />
        <activity android:name=".enter">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />

        <activity
            android:name="com.facebook.FacebookActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/app_name" />
        <activity
            android:name="com.facebook.CustomTabActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:scheme="@string/fb_login_protocol_scheme" />
            </intent-filter>
        </activity>
    </application>

</manifest>